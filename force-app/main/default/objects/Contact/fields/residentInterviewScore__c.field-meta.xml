<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>residentInterviewScore__c</fullName>
    <description>Empty ratings are treated as 0</description>
    <externalId>false</externalId>
    <formula>(
interviewMarketability__c 
+interviewOwnership__c 
+interviewFervorForOpportunity__c
) / 3</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Average of Interview Ratings</inlineHelpText>
    <label>Interview Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
