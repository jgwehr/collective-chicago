<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Year_Multiplier_more_than_0</fullName>
    <active>true</active>
    <description>Income values must be non-negative and non-zero</description>
    <errorConditionFormula>incomeForecastedYearsMultipler__c &lt; 1</errorConditionFormula>
    <errorDisplayField>incomeForecastedYearsMultipler__c</errorDisplayField>
    <errorMessage>Multipler must be greater than zero</errorMessage>
</ValidationRule>
