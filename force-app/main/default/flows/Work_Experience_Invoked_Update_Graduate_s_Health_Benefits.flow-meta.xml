        <label>Loop Work Experiences</label>
        <locationX>314</locationX>
        <locationY>782</locationY>
        <collectionReference>getWorkExperiences</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>checkHealthBenefits</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>foundWorkExperiences</targetReference>
        </noMoreValuesConnector>
    </loops>
